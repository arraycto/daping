
.home-header
  .menu-list
    margin-left 50px
    margin-right 50px
    height 100%
    float left
    ul
      display flex
      height 100%
      list-style-type none
      //padding 0 30px
      li
        height 100%
        box-sizing border-box
        font-size 14px
        padding 0 30px
        &.current
          a
            color #5799FE
            border-bottom 3px solid #5799FE
            .icon
              background-image url("data:image/png;base64,")

        &:nth-child(2)
          a
            .icon
              background-image url("data:image/png;base64,")
          &.current
            a
              .icon
                background-image url("data:image/png;base64,")
        a
          display flex
          height 100%
          align-items center
          color #515C6B
          text-decoration none
          box-sizing border-box
          padding 0 5px
          i
            margin-right 4px
            width 12px
            height 12px
          .icon
            background-size 12px
            background-position 0 0
            background-repeat no-repeat
            background-image url("data:image/png;base64,")
          span{
            line-height 1
          }
